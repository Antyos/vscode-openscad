{
    "name": "openscad",
    "displayName": "OpenSCAD",
    "description": "OpenSCAD language support for VSCode",
    "version": "0.0.1",
    "publisher": "Antyos",
    "license": "SEE LICENSE IN LICENSE.txt",
    "icon": "https://github.com/Antyos/vscode-openscad/blob/master/images/logo.png",
    "homepage": "https://github.com/Antyos/vscode-openscad/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/Antyos/vscode-openscad/issues"
    },
    "repository": 
    {
        "type": "git",
        "utl": "https://github.com/Antyos/vscode-openscad"
    },
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "scad",
                "aliases": [
                    "OpenSCAD",
                    "openscad",
                    "scad"
                ],
                "extensions": [
                    ".scad"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "scad",
                "path": "./snippets/snippets.json"
            }
        ],
        "grammars": [
            {
                "language": "scad",
                "scopeName": "source.scad",
                "path": "./syntaxes/scad.tmlanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1"
    },
    "dependencies": {
        "vsce": "^1.73.0"
    }
}
